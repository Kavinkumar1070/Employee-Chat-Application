{
    "create new employee detail": {
        "project": "create new employee detail",
        "project description": "Create a new personal employee record with provided details.",
        "url": "http://127.0.0.1:8000/admin/employees/{employee_id}",
        "method": "POST",
        "payload": {
            "firstname": {
                "description": "First name of the employee",
                "datatype": "string",
                "required": true,
                "default": "None"
            },
            "lastname": {
                "description": "Last name of the employee",
                "datatype": "string",
                "required": true,
                "default": "None"
            },
            "dateofbirth": {
                "description": "Date of birth of the employee in YYYY-MM-DD format",
                "datatype": "date",
                "format": "%Y-%m-%d",
                "required": true,
                "default": "None"
            },
            "contactnumber": {
                "description": "Contact number of the employee",
                "datatype": "mobile",
                "required": true,
                "default": "None"
            },
            "emailaddress": {
                "description": "Email address of the employee",
                "datatype": "regex",
                "format": "^[a-z0-9]+@[a-z0-9]+\\.com$",
                "required": true,
                "default": "None"
            },
            "address": {
                "description": "Residential address of the employee",
                "datatype": "string",
                "required": true,
                "default": "None"
            },
            "nationality": {
                "description": "Nationality of the employee",
                "datatype": "string",
                "required": true,
                "default": "None"
            },
            "gender": {
                "description": "Gender of the employee",
                "datatype": "choices",
                "required": true,
                "choices": ["male", "female", "other"],
                "default": "None"
            },
            "maritalstatus": {
                "description": "Marital status of the employee",
                "datatype": "choices",
                "required": true,
                "choices": ["single", "married", "divorced", "widowed"],
                "default": "None"
            }
        }
    },
